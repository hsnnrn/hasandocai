{
  "version": 2,
  "buildCommand": "npm run build:renderer",
  "outputDirectory": "dist/renderer",
  "devCommand": "npm run dev:renderer",
  "installCommand": "npm install --ignore-scripts --no-optional --legacy-peer-deps || npm install --ignore-scripts --no-optional",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "SKIP_ELECTRON_BUILDER": "true",
    "SKIP_NATIVE_DEPS": "true",
    "SKIP_POSTINSTALL": "true"
  }
}

